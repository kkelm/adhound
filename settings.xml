<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                          https://maven.apache.org/xsd/settings-1.0.0.xsd">
    <localRepository/>
    <interactiveMode/>
    <offline/>
    <pluginGroups>
        <pluginGroup>org.junit.platform</pluginGroup>
    </pluginGroups>
    <servers/>
    <mirrors/>
    <proxies/>
    <profiles>
        <profile>
            <id>adhound</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <junit-platform-runner-version>1.6.0</junit-platform-runner-version>
                <junit-vintage-version>5.6.0</junit-vintage-version>
                <jupiter-version>5.6.0</jupiter-version>
                <junit.version>4.13</junit.version>
                <javax.version>8.0.1</javax.version>
                <mysql-version>8.0.18</mysql-version>
                <jstl-version>1.2</jstl-version>
                <log4j-version>2.13.0</log4j-version>
                <maven-compiler-plugin-version>3.8.1</maven-compiler-plugin-version>
                <maven-surefire-plugin-version>3.0.0-M4</maven-surefire-plugin-version>
                <junit-platform-runner-version>1.3.2</junit-platform-runner-version>
                <mybatis-version>3.5.3</mybatis-version>
            </properties>
        </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>adhound</activeProfile>
    </activeProfiles>
</settings>